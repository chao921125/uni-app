{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
				"enablePullDownRefresh": false
      }
    }
  ],
	"subPackages": [{
		"root": "pages/common/",
		"pages": [{
			"path": "blank",
			"style": {
				"navigationBarTitleText": "空白",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "loading",
			"style": {
				"navigationBarTitleText": "加载",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "web",
			"style": {
				"navigationBarTitleText": "WEB",
				"enablePullDownRefresh": true
			}
		}]
	}, {
		"root": "pages/demo/",
		"pages": [{
			"path": "language",
			"style": {
				"navigationBarTitleText": "国际化",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "lottery",
			"style": {
				"navigationBarTitleText": "抽奖",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "lottery-basic",
			"style": {
				"navigationBarTitleText": "抽奖（基础）",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "lucky-wheel",
			"style": {
				"navigationBarTitleText": "抽奖（转盘）",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "lucky-grid",
			"style": {
				"navigationBarTitleText": "抽奖（九宫格）",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "lucky-machine",
			"style": {
				"navigationBarTitleText": "抽奖（老虎机）",
				"enablePullDownRefresh": true
			}
		}]
	}],
	"preloadRule": {
		"pages/index/index": {
			"network": "all",
			"packages": ["__APP__"]
		}
	},
  "easycom": {
    "autoscan": true,
    "custom": {}
  },
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "CC NET",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "backgroundTextStyle": "dark",
